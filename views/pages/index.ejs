	<!-- HOME BANNER TEXT/ FIELDS -->

	<section class="map-banner">
		<div class="constrainer">
			<div class="map-container">
				<svg id="svgA" width="100%" height="400px" viewBox="0 0 1015 426.01" preserveAspectRatio="xMidYMid meet"></svg>
				<script>


				///////  SVG A  ///////////

				var snapA = Snap("#svgA");

				  // SVG A - "Squiggly" Path
				  var myPathA = snapA.path("M100.22,268.46a26.87,26.87,0,0,0,21.94-2.71c1.75-1.08,3.42-2.39,5.42-2.85,3.9-.91,7.68,1.57,11,3.74a104.85,104.85,0,0,0,70.31,15.66c5.22-6.9,16.4-8.21,23.07-2.71,3.09,2.55,5.14,6.17,8.17,8.8,6.86,6,17.18,5.57,26,3.26s17.34-6.22,26.43-6.26c16.83-.06,31.24,13.08,48,14.52,9,0.78,17.94-1.9,27-2.56,19.58-1.44,38.59,6.6,58.15,8.17,26.56,2.14,56.22-7,78.16,8.13,2.44,1.68,4.89,3.7,7.84,3.93a14.09,14.09,0,0,0,4.26-.54l97.67-22.6").attr({
						  id: "squiggle",
						  fill: "none",
				  		});

				  // Draw Path
				  var lenA = myPathA.getTotalLength();

				  // Animate Path
				  myPathA.attr({
					  stroke: '#fff',
					  strokeWidth: 4,
					  strokeLinecap: "round",
					  strokeLinejoin: "round",
					  strokeDasharray: "0 7",
				      fill: 'none',
				  
				  }).animate({"stroke-dashoffset": 0}, 9000);

				  // Truck
				  var truckA = snapA.image("../../img/red-truck5.svg","","",18,18);
				  truckA.attr({
				    fill: "#E14339"
				  });

				  setTimeout( function() {
				  	// truck start point
				    Snap.animate(200, lenA, function( value ) {
				       movePoint = myPathA.getPointAtLength( value );
				       // move truck along path via x & y attributes
				       truckA.attr({ x: movePoint.x-9, y: movePoint.y-9 }); 
				    // speed of truck
				    },120000);
				  });


				///////  SVG B  ///////////

				var snapB = Snap("#svgA");

				  // SVG B - "Squiggly" Path
				  var myPathB = snapB.path("M96.52,272.11C91.44,257.67,73.48,253,62.83,242c-11.13-11.5-13.29-28.9-13-44.9s2.3-32.46-2.66-47.68c-2.19-6.74-5.7-13-7.57-19.84-5.1-18.63,4-40.17,20.85-49.54,3.08-1.71,6.38-3.05,9.31-5,2.77-1.87,5.14-4.26,7.72-6.39A50.64,50.64,0,0,1,114,57.33").attr({
						  id: "squiggle",
						  fill: "none",
				  		});

				  // Draw Path
				  var lenB = myPathB.getTotalLength();

				  // Animate Path
				  myPathB.attr({
					  stroke: '#fff',
					  strokeWidth: 4,
					  strokeLinecap: "round",
					  strokeLinejoin: "round",
					  strokeDasharray: "0 7",
				      fill: 'none',

				  }).animate({"stroke-dashoffset": 0}, 9000);

				  // Truck
				  var truckB = snapB.image("../../img/red-truck5.svg","","",18,18);
				  truckB.attr({
				    fill: "#E14339"
				  });

 				  // Pin Socal
				  var pinSocal = snapB.image("../../img/pinSocal.svg");
				  pinSocal.attr({
				    fill: "#FFF",
				  });

				  // Pin Or
				  var pinOr = snapB.image("../../img/pinOr.svg");
				  pinOr.attr({
				    fill: "#FFF",
				  });

				  setTimeout( function() {
				  	// truck start point
				    Snap.animate(50, lenB, function( value ) {
				       movePoint = myPathB.getPointAtLength( value );
				       // move truck along path via x & y attributes
				       truckB.attr({ x: movePoint.x-9, y: movePoint.y-9 }); 
				    // speed of truck
				    },120000);
				  });


				///////  SVG C  ///////////

				var snapC = Snap("#svgA");

				  // SVG C - "Squiggly" Path
				  var myPathC = snapC.path("M514.8,214.2C494,195.66,460.13,193.84,437.42,210c-7.13,5.08-13.4,11.79-21.72,14.52-5.54,1.82-11.51,1.7-17.35,1.55l-42.25-1a89.72,89.72,0,0,1-16-1.35c-10.61-2.19-20-8.12-30.17-11.91-31.95-12-67.43-2-101.54-2.5-11.06-.16-22.38-1.4-33,1.68-21.48,6.23-34.07,27.87-44.5,47.66").attr({
						  id: "squiggle",
						  fill: "none",
				  		});

				  // Draw Path
				  var lenC = myPathC.getTotalLength();

				  // Animate Path
				  myPathC.attr({
					  stroke: '#fff',
					  strokeWidth: 4,
					  strokeLinecap: "round",
					  strokeLinejoin: "round",
					  strokeDasharray: "0 7",
				      fill: 'none',

				  }).animate({"stroke-dashoffset": 0}, 9000);

				  // Truck
				  var truckC = snapC.image("../../img/red-truck5.svg","","",18,18);
				  truckC.attr({
				    fill: "#FFF"
				  });

				  // Pin KA
				  var pinC = snapC.image("../../img/pinKa.svg");
				  truckB.attr({
				    fill: "#FFF",
				  });

				  // Pin AR
				  var pinC = snapC.image("../../img/pinAr.svg");
				  truckB.attr({
				    fill: "#FFF",
				  });


				  setTimeout( function() {
				  	// truck start point
				    Snap.animate(50, lenC, function( value ) {
				       movePoint = myPathC.getPointAtLength( value );
				       // move truck along path via x & y attributes
				       truckC.attr({ x: movePoint.x-9, y: movePoint.y-9 }); 
				    // speed of truck
				    },120000);
				  });


				</script>
			</div>
		</div>
	</section>



